    "# Example of plotting two boxplots side by side\n",
    "\n",
    "hsi_2016 = stocks.loc[stocks['Year']==2016, ['^HSI']].values\n",
    "hsi_2017 = stocks.loc[stocks['Year']==2017, ['^HSI']].values\n",
    "hsi_2018 = stocks.loc[stocks['Year']==2018, ['^HSI']].values\n",
    "hsi_2019 = stocks.loc[stocks['Year']==2019, ['^HSI']].values\n",
    "hsi_2020 = stocks.loc[stocks['Year']==2020, ['^HSI']].values\n",
    "\n",
    "#hsi_2016\n",
    "#hsi_2017\n",
    "#hsi_2018\n",
    "#hsi_2019\n",
    "#hsi_2020\n",
    "\n",
    "hsi = hsi_2016, hsi_2017, hsi_2018, hsi_2019, hsi_2020\n",
    "\n",
    "fig, ax = plt.subplots(1, 1, figsize = (12, 6))\n",
    "\n",
    "plt.boxplot(np.array([hsi_2016, hsi_2017, hsi_2018, hsi_2019, hsi_2020], dtype='object'), labels=['2016', '2017', '2018', '2019', '2020']);\n",
    "plt.title('Hang Seng Index 2016-2020');"
