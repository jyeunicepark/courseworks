   "source": [
    "shares = {'Towngas':5000, 'HSBC':1000, 'HKEX':300, 'Tencent':200, 'PetroChina':10000}\n",
    "\n",
    "share_list = []\n",
    "\n",
    "for i in range(len(stocks)): \n",
    "    towngas = shares.get('Towngas')*stocks[\"Towngas\"][i]\n",
    "    hsbc = shares.get('HSBC')*stocks['HSBC'][i]\n",
    "    hkex = shares.get('HKEX')*stocks['HKEX'][i]\n",
    "    tencent = shares.get('Tencent')*stocks['Tencent'][i]\n",
    "    petrochina = shares.get('PetroChina')*stocks['PetroChina'][i]\n",
    "    total = towngas + hsbc + hkex + tencent + petrochina\n",
    "    share_list.append(total)\n",
    "    \n",
    "\n",
    "# Example of setting major ticks and rotate axis labels\n",
    "x = ['Sunday', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday']\n",
    "\n",
    "\n",
    "fig, ax = plt.subplots(figsize= (25, 10))\n",
    "\n",
    "ax.plot(stocks.Date, share_list)\n",
    "\n",
    "\n",
    "xticks= np.arange(30, 20, 200) \n",
    "ax.set_xticks(xticks);\n",
    "\n",
    "xlabels= x \n",
    "ax.xaxis.set_major_locator(plt.MaxNLocator(10));\n",
    "plt.xticks(rotation = 45);\n"
   ]
