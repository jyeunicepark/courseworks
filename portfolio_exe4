     "output_type": "display_data"
    }
   ],
   "source": [
    "# Example of plotting a stacked bar\n",
    "\n",
    "def ld_2016(i):\n",
    "    return float(stocks.loc[stocks['Date']=='2016-12-30'][i]*(shares.get(i)))\n",
    "\n",
    "def ld_2017(i):\n",
    "    return float(stocks.loc[stocks['Date']=='2017-12-29'][i]*(shares.get(i)))\n",
    "\n",
    "def ld_2018(i):\n",
    "    return float(stocks.loc[stocks['Date']=='2018-12-28'][i]*(shares.get(i)))\n",
    "\n",
    "def ld_2019(i):\n",
    "    return float(stocks.loc[stocks['Date']=='2019-12-27'][i]*(shares.get(i)))\n",
    "\n",
    "def ld_2020(i):\n",
    "    return float(stocks.loc[stocks['Date']=='2020-12-25'][i]*(shares.get(i)))\n",
    "\n",
    "\n",
    "#2016\n",
    "l_2016 = [ld_2016('Towngas'), ld_2016('HSBC'), ld_2016('HKEX'), ld_2016('Tencent'), ld_2016('PetroChina')]\n",
    "\n",
    "#2017\n",
    "l_2017 = [ld_2017('Towngas'), ld_2017('HSBC'), ld_2017('HKEX'), ld_2017('Tencent'), ld_2017('PetroChina')]\n",
    "ld_sum_2017 = ld_2017('Towngas')+ld_2017('HSBC')+ld_2017('HKEX')+ld_2017('Tencent')+ld_2017('PetroChina')\n",
    "\n",
    "#2018\n",
    "l_2018 = [ld_2018('Towngas'), ld_2018('HSBC'), ld_2018('HKEX'), ld_2018('Tencent'), ld_2018('PetroChina')]\n",
    "ld_sum_2018 = ld_2018('Towngas')+ld_2018('HSBC')+ld_2018('HKEX')+ld_2018('Tencent')+ld_2018('PetroChina')\n",
    "\n",
    "#2019\n",
