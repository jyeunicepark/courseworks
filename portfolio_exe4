    "#stocks.tail(5)\n",
    "\n",
    "shares = {'Towngas':5000, 'HSBC':1000, 'HKEX':300, 'Tencent':200, 'PetroChina':10000}\n",
    "\n",
    "last_day = stocks.loc[stocks['Date']=='2020-12-25']\n",
    "\n",
    "towngas = last_day.Towngas * 5000\n",
    "hsbc = last_day.HSBC * 1000\n",
    "hkex = last_day.HKEX * 300\n",
    "tencent = last_day.Tencent * 200\n",
    "petro = last_day.PetroChina * 10000\n",
    "\n",
    "towngas = int(towngas)\n",
    "hsbc = int(hsbc)\n",
    "hkex = int(hkex)\n",
    "tencent = int(tencent)\n",
    "petro = int(petro)\n",
    "\n",
    "\n",
    "stock_labels = ['Towngas', 'HSBC', 'HKEX', 'Tencent', 'Petro China']\n",
    "y = [towngas, hsbc, hkex, tencent, petro]\n",
    "\n",
    "\n",
    "#Pie Chart\n",
    "fig, ax = plt.subplots(figsize=(11, 8));\n",
    "\n",
